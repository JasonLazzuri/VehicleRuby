<h1 id="jd">J&D's Automotives</h1>

<form action="/vehicles_list" method="post">
  <div class="form-group">
    <label for="name">Add a vehicle</label>
    <input id="make" placeholder="Make" name="make" class="form-control" type="text">
    <input id="model" placeholder="Model" name="model" class="form-control" type="text">
    <input id="year" placeholder="Year" name="year" class="form-control" type="text">
    <button type="submit" class="btn">Add</button>
  </div>
</form>
